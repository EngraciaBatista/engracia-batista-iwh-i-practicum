doctype html
html
  head
    title= title
    link(rel='stylesheet', href='/css/style.css')
  body
    h1 Custom Objects List
    a(href='/update-cobj') Add to this table
    table
      thead
        tr
          th Pet Name
          th Pet Type
          th Pet Age
      tbody
        each item in data
          tr
            td= item.properties.pet_name
            td= item.properties.pet_type
            td= item.properties.pet_age
    a(href='/') Return to the homepage
